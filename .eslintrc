{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "func-names": 0,
    "global-require": 0,
    "no-console": 0,
    "quotes": [
      "error",
      "double"
    ],
    "quote-props": [
      "error",
      "consistent"
    ]
  },
  "globals": {
    "appRequire": true
  }
}
